<p id="notice"><%= notice %></p>

<h1>Staff Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @staff_types.each do |staff_type| %>
      <tr>
        <td><%= link_to 'Show', staff_type %></td>
        <td><%= link_to 'Edit', edit_staff_type_path(staff_type) %></td>
        <td><%= link_to 'Destroy', staff_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff Type', new_staff_type_path %>
